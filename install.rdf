<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>zoterowechat@zotero.org</em:id>
		<em:name>Zotero WeChat</em:name>
		<em:version>0.0.1</em:version>
		<em:description>为Zotero Standalone提供微信公众号快照支持</em:description>
		<em:creator>zsx</em:creator>
		<em:updateURL>https://www.zotero.org/download/plugins.rdf</em:updateURL>
		<em:type>2</em:type> <!-- type=extension -->
		
		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>3.0</em:minVersion>
				<em:maxVersion>4.0.*</em:maxVersion>
			</Description>
		</em:requires>
    
		<!-- Requires Zotero Standalone -->
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>3.0</em:minVersion>
				<em:maxVersion>5.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
